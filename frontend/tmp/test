<script>
	function setCookie(c_name, value, exdays) {
		var exdate = new Date();
		exdate.setDate(exdate.getDate() + exdays);
		var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
		document.cookie = c_name + "=" + c_value;
	}

	function hasCookie(c_name) {
		var cookies = document.cookie.split('; ');
		var hasCookie = false;
		for (var i = 0; i < cookies.length; i++) {
			if (cookies[i].split('=')[0] === c_name) {
				hasCookie = true;
				break;
			}
		}
		return hasCookie;
	}

	var placeholderAnimInterval = null;
	var pausePlaceholderAnim = false;

	function PlaceholderAnim() {
		var $target = $('.search-query');
		$target.attr("placeholder", "");
		var animtext = ["", "", ""];
		var currentText = "";
		var textCounter = 0;
		var prevIntCount = 0;
		jQuery({
			count: 0
		}).delay(2500).animate({
			count: animtext[0].length
		}, {
			duration: animtext[0].length * 150,
			step: function () {
				$target.attr("placeholder", currentText);
				if (prevIntCount == textCounter && textCounter < animtext[0].length) {
					currentText += animtext[0][textCounter];
					textCounter++;
				}
				else if (textCounter == animtext[0].length) {
					textCounter = 0;
				}
				prevIntCount = Math.round(this.count);
			},
			complete: function () {
				currentText = "";
				jQuery({
					count: 0
				}).delay(animtext[0].length * 150 + 1000).animate({
					count: animtext[1].length
				}, {
					duration: animtext[1].length * 150,
					step: function () {
						$target.attr("placeholder", currentText);
						if (prevIntCount == textCounter && textCounter < animtext[1].length) {
							currentText += animtext[1][textCounter];
							textCounter++;
						}
						else if (textCounter == animtext[1].length) {
							textCounter = 0;
						}
						prevIntCount = Math.round(this.count);
					},
					complete: function () {
						currentText = "";
						jQuery({
							count: 0
						}).delay(animtext[1].length * 150 + 1000).animate({
							count: animtext[2].length
						}, {
							duration: animtext[2].length * 150,
							step: function () {
								$target.attr("placeholder", currentText);
								if (prevIntCount == textCounter && textCounter < animtext[2].length) {
									currentText += animtext[2][textCounter];
									textCounter++;
								}
								else if (textCounter == animtext[2].length) {
									textCounter = 0;
								}
								prevIntCount = Math.round(this.count);
							},
							complete: function () {
								currentText = "";
								placeholderAnimInterval = setTimeout(PlaceholderAnim, 5000);
							}
						});
					}
				});
			}
		});
	}

	var modalShown = false;
	var queryEntered = false;
	var prevQuery = "";
	var lastQuery = "";
	var mY = 0;
	var movedToTop = false;
	var horzAcceptable = false;
	var mTravelDistance = 0;
	var travelDistanceThreshold = 9000000;
	var popinTimeThreshold = 7000;
	var popupEligible = false;
	$(function () {
		$(".aspNetHidden").remove();

		$('.search-query').focus();

		PlaceholderAnim();

		$("body").mousemove(function (e) {
			movedToTop = e.pageY < mY;
			mY = e.pageY;
			mTravelDistance += (e.pageY + e.pageX) * (e.pageY + e.pageX);
			if (mTravelDistance >= travelDistanceThreshold) {
				popupEligible = true;
				mTravelDistance = 0;
			}
		}).mouseleave(function (e) {
			horzAcceptable = e.pageX >= 0 && e.pageX <= $(window).width();
			if (!modalShown && movedToTop && horzAcceptable && popupEligible) {
				modalShown = true;
				horzAcceptable = false;
				movedToTop = false;
				$("#modal-Leaving").modal("show");
				_gaq.push(['_trackEvent', 'JerusalemSearch', 'Leaving already popup', 'shown']);
			}
		});

		$(".modal").on('shown', function () {
			$(this).find("input[type=search]").eq(0).focus();
		});

		$(".search-submit").click(function (e) {
			var prevQuery = "";
			var $siblingQuery = $(this).parents(".search-h").eq(0).find('.search-query');
			var val = $siblingQuery.val();
			if (val == "") {
				e.preventDefault();
			}
			else if (prevQuery !== "") {
				if (val !== prevQuery) {
					_gaq.push(['_trackEvent', 'JerusalemSearch', 'query submitted', 'prev query: "' + prevQuery + '"; new query: "' + val + '"']);
				}
				else {
					_gaq.push(['_trackEvent', 'JerusalemSearch', 'query resubmitted', 'query: "' + prevQuery + '"']);
				}
			}
			else {
				_gaq.push(['_trackEvent', 'JerusalemSearch', 'query submitted', 'query: "' + val + '"']);
			}
		});

		$('.search-query').keyup(function () {
			var val = encodeURI($(this).val());
			var baseUrl = "//search.jerusalem.com/y/8";
			var baseParams = $("#urlParams").val();
			var $allSearchQueries = $('.search-query');
			if ($(this).val() != lastQuery) {
				$allSearchQueries.not(this).val($(this).val());
			}
			//update category links
			$('.categories a.search-category').each(function (e) {
				var newHref = baseUrl + "/" + $(this).attr('data-search-category') + "/" + "?q=" + val + baseParams;
				$(this).attr('href', newHref);
			});
			if (!queryEntered) {
				_gaq.push(['_trackEvent', 'JerusalemSearch', 'query changed', 'front page']);
				queryEntered = true;
				//change the arrow
				$("#action-message").fadeOut();

				$("#action-message-button").delay(1000).fadeIn({
					easing: 'swing', duration: '500', complete: function () {
						$("#action-message-button .arrow").fadeIn();
					}
				});
			}
			lastQuery = $(this).val();
			
		});

		$(".categories a.search-category").click(function (e) {
			e.preventDefault();
			var q = $('.search-query').val();
			if (q.trim() === "") {
				$(".categories a.search-category").each(function () {
					$(this).removeClass("highlight");
				});
				$(this).addClass("highlight");
				$("#aspnetForm").attr("action", "//search.jerusalem.com/y/8/" + $(this).attr('data-search-category') + "/");
				$('.search-query').focus();
			}
			else {
				window.location.href = $(this).attr("href");
			}
		});

		$(".suggestions a").click(function () {
			var term = $(this).text();
			_gaq.push(['_trackEvent', 'JerusalemSearch', 'suggestion link clicked', ' term: "' + term + '"']);
		});

		$("#body").click(function (e) {
			$('.search-query').focus();
		});

		//$("#init-search-provider").click(function (e) {
		//	e.preventDefault();
		//	if (typeof window.external.AddSearchProvider !== "undefined") {
		//		window.external.AddSearchProvider("http://search.jerusalem.com/JerusalemWebSearch.xml");
		//	}
		//});

		$(".hotels-link").click(function () {
			_gaq.push(['_trackEvent', 'JerusalemSearch', 'hotels link clicked', 'main page']);
		});

		$('#init-homepage').click(function (e) {
			e.preventDefault();
			this.style.behavior = "url('#default#homepage')";
			_gaq.push(['_trackEvent', 'JerusalemSearch', 'user clicked set homepage link', 'front page']);
			if (typeof this.setHomePage !== 'undefined') {
				//add this page as homepage
				this.setHomePage('search.jerusalem.com/y/home');
			} else {
				var ua;
				//show instructions popup
				//show for chrome
				if ($.browser.chrome) {
					ua = "chrome";
				}
					//show for safari
				else if ($.browser.webkit) {
					ua = "safari";
				}
					//show for firefox
				else if ($.browser.mozilla) {
					ua = "firefox";
				}
				if (ua && $(".homepage-instruction-box." + ua).length > 0) {
					$(".homepage-instruction-box." + ua).show();
					$('#modal-homepage').modal('show');
				}
				else {
					console.log('unrecognized user agent');
				}
			}
			setCookie("DailyPhoto", 1, 365);
			$("#init-homepage-wrapper").removeClass("blink");
			$("#action-message").fadeOut();
		});

			$("#action-message").delay(1000).fadeIn({
				easing: 'swing', duration: '500', complete: function () {
					$("#action-message .arrow").fadeIn();
				}
			});

		var popinTimeout = setTimeout(function () { popupEligible = true; }, popinTimeThreshold);
	});
</script>




